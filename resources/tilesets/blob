.unusedcode {
    color: var(--yellow-verydark);
}</style><script type="text/javascript">function updateWarningTips() {
    const coll3 = document.getElementsByClassName("warntip");

    let winwidth = document.documentElement.clientWidth;

    // A tooltip is positioned relative to the middle of the parent, as we have "left: 50%;" in the CSS.
    // By default it "hangs to the right" from there.
    // We want to center it, *but* keep it within the boundaries of the window...
    for (i = 0; i < coll3.length; i++) {
        let element = coll3[i]
        let elemWidth = element.offsetWidth
        let margin = -element.offsetWidth / 2;

        let parent = element.parentElement;
        let parentCenter = parent.offsetLeft + parent.offsetWidth / 2;

        // First adjust the right edge...
        let rightEdge = parentCenter + elemWidth + margin
        if (rightEdge > winwidth - 10) {
            margin -= (rightEdge - (winwidth - 10))
        }

        // ...*then* adjust the left edge, as seeing the leftmost part of the tooltip is more important
        let leftEdge = parentCenter + margin
        if (leftEdge < 10) {
            margin += 10 - leftEdge
        }

        element.style.marginLeft = margin + "px";
    }

    console.log("Updated tooltip positions for " + coll3.length + " elements")
}

window.onload = function () {
    const coll = document.getElementsByClassName("blob_collapser");
    let i;

    for (i = 0; i < coll.length; i++) {
        coll[i].addEventListener("click", function () {
            this.classList.toggle("active");
            const content = this.parentElement.nextElementSibling;
            if (content.style.display === "block") {
                content.style.display = "none";
            } else {
                content.style.display = "block";
            }
        });
    }

    console.log("Added collapsers to " + coll.length + " elements")

    const coll2 = document.getElementsByClassName("blob_hider");

    for (i = 0; i < coll2.length; i++) {
        coll2[i].addEventListener("click", function () {
            console.log("Hiding " + this.className)
            this.classList.toggle("active");
            for (const cls of this.classList) {
                // noinspection JSUnfilteredForInLoop
                console.log(cls)
                if (cls.startsWith("hide-")) {
                    console.log("Hiding " + cls)
                    // noinspection JSUnfilteredForInLoop
                    const rest = cls.substr(5)
                    const toChange = document.getElementsByClassName("warn-" + rest);
                    for (const warning of toChange) {
                        warning.style.display = "none"
                    }
                }
            }
        });
    }

    console.log("Added hiders to " + coll2.length + " elements")

    let grade;
    for (grade = 3; grade <= 5; grade++) {
        let toggler = document.getElementById("toggle" + grade)
        const togglables = document.getElementsByClassName("grade" + grade);
        toggler.addEventListener("click", function () {
            for (i = 0; i < togglables.length; i++) {
                togglables[i].classList.toggle("hidden")
            }
            toggler.classList.toggle("inactive")
        })
        console.log("Added grade togglers for " + togglables.length + " elements")
    }


    // Must be updated when we resize the window since this leads to new positions and margins...
    window.addEventListener("resize", updateWarningTips)
    updateWarningTips()

}</script></head><body>

